<%= form_for @student do |f| %>
  <div class="field">
    <%= f.label :Studentname %>
    <%= f.text_field :name, :size => 140 %>
    <br>
    <%= f.label :Profile %>
    <%= f.text_field :profile, :size => 140 %>
    <br>
    <%= f.label :mentor %>
    <%= f.text_field :mentor, :size => 140 %>
    <br>
    <%= f.label :grade %>
    <%= f.text_field :grade, :size => 140 %>
  </div>
  <%= f.submit "Tweetする" %>
  
<% end %>
